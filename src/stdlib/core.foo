;; Utils:
(define (empty? lst)
  (nil? lst))

(define (id x) x)

(define &gensym-counter (ref 0))
(define (gensym root)
  (set! &gensym-counter (+ 1 (deref &gensym-counter)))
  (string->symbol (string-append (symbol->string (symbol->llvm root))
                                 (number->string &gensym-counter))))
